<?xml version="1.0" encoding="utf-8" ?>
<odoo>
    <data>
        <record id="member_tree" model="ir.ui.view">
            <field name="name">member.tree</field>
            <field name="model">member</field>
            <field name="arch" type="xml">
                <tree>
                    <field name="name"/>
                    <field name="first_name"/>
                    <field name="last_name"/>
                    <field name="title"/>
                    <field name="hire_date"/>
                    <field name="birth_date"/>
                    <field name="phone" invisible="1"/>
                    <field name="gender" invisible="1"/>
                    <field name="department_name" invisible="1"/>
                    <field name="employee_key" invisible="1"/>
                    <field name="parent_employee_key" invisible="1"/>
                </tree>
            </field>
        </record>
    </data>
</odoo>